pico-8 cartridge // http://www.pico-8.com
version 43
__lua__
gpio = {}
setmetatable(gpio, {
 __index = function(_, i)
  return peek(0x5f80 + i)
 end,
 __newindex = function(_, i, v)
  poke(0x5f80 + i, v)
 end
})

function _update()
 x = gpio[0]      -- send to js
 y = gpio[1]  -- read from js
end

function _draw()
	cls()
	print("gpio 0 "..x, 0, 0, 7)
	print("gpio 1 "..y, 0, 8, 7)
	pset(x, y, 8)
end
