version 43
__lua__
-- safe gpio wrapper
gpio = {}
setmetatable(gpio, {
    __index = function(_, i)
        return peek(0x5f80 + i)
    end,
    __newindex = function(_, i, v)
        poke(0x5f80 + i, v)
    end
})

function _update()
    gpio[0] = 123      -- send to js
    v = gpio[1]  -- read from js
end

function _draw()
				cls()
				print("gpio 0 "..gpio[0], 0, 0)
				print("gpio 1 "..v, 0, 16)
end
